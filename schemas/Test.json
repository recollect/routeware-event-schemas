{
	"definitions": {},
        "$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "com.routeware.eventbus.Test", 
	"title": "Test Event", 
	"type": "object",
	"required": [
		"message"
	],
	"properties": {
		"message": {
			"$id": "message", 
			"title": "Message", 
			"type": "string",
			"default": "",
			"examples": [
				"foobar"
			],
			"pattern": "^.*$"
		},
		"fail": {
			"$id": "fail",
			"title": "Should Fail?",
			"type": "number",
			"default": 0
		}
	}
}
